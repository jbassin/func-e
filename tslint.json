{
   "extends": ["tslint:recommended", "tslint-config-prettier"],
   "rules": {
      "object-literal-sort-keys": {
         "severity": "off"
      },
      "no-console": {
         "severity": "off"
      },
      "no-unused-expression": {
         "severity": "off"
      },
      "variable-name": {
         "severity": "off"
      }
   }
}
